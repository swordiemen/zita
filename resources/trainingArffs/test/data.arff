cording to the mood of the bunny    ellipse(posx + 23, posy - 115, 12, 40);    // draws inside of the ear that shifts colour according to the mood of the bunny    ellipse(posx - 23, posy - 115, 12, 40);    // tummy that shifts colour (not sure if I want this in it looks odd)    ellipse(posx - 50, posy - 39, 70, 30);    // tail    ellipse(posx - 84, posy - 64, 23, 23);    // mouth sad    image(mouth, posx - 3, posy - 55, 17, 10);    if (carrotX < posx + 50 && carrotX > posx - 50 && carrotY < posy - 20 && carrotY > posy - 70 && carrot.isVisable()) {        // if the carrot is near the rabbit        mouth = mouth2;    } else {        mouth = mouth1;    }// this statements makes the bunny smile or be angry according to the mood of the bunny}',?
