'class Ghost {    float x;    float y;    float xspeed;    float yspeed;    int bodyColor;    Ghost(float overX, float overY) {        x = overX;        y = overY;        bodyColor = color(random(100, 255));    }    void display() {        ellipseMode(CENTER);        rectMode(CENTER);        // body        fill(bodyColor);        noStroke();        triangle(x - 25, y - 50, x, y + 37.5f, x + 25, y - 50);        // head        noFill();        stroke(bodyColor);        strokeWeight(5);        ellipse(x, y - 75, 37.5f, 37.5f);        strokeWeight(2);        ellipse(x - 7.5f, y - 80, 5, 2.5f);        ellipse(x + 7.5f, y - 80, 5, 2.5f);        // Leftarm        fill(bodyColor);        noStroke();        rect(x + 30, y - 27.5f, 7.5f, 42.5f, 7.5f);        // Rightarm        fill(bodyColor);        noStroke();        rect(x - 30, y - 27.5f, 7.5f, 42.5f, 7.5f);    }    void update() {        // auto-movement        y = y - 1;        if (y < -50) {            y = 575;        }    }    // changing colour    void bully(float overX, float overY) {        if (isOver(overX, overY)) {            bodyColor = backgroundColor;            // mouth            fill(bodyColor);            ellipse(x, y - 67.10f, 10, 5);        } else            bodyColor = color(230);    }    // check if over ghost    boolean isOver(float overX, float overY) {        boolean isOverGhost = false;        if (pow(overX - x, 2) / (75 * 75) + pow(overY - y, 2) / (50 * 75) <= 1) {            isOverGhost = true;        }        return isOverGhost;    }}',?
