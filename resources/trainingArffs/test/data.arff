eng);        stroke(1);        triangle(posX - 5, posY - 50, posX + 5, posY - 50, posX, posY - 38);        // eyes        fill(bodyPeng);        ellipse(posX - 8, posY - 57, bodyX - 65, bodyY - 75);        ellipse(posX + 8, posY - 57, bodyX - 65, bodyY - 75);    }    void move() {        if (mousePressed == true) {            locked = true;            posX = mouseX;            posY = mouseY;        } else {            locked = false;        }        x = x + (xspeed * xdirection);        y = y + (yspeed * ydirection);        if (x > width - bodyX - 50 || x < bodyX + 40) {            xdirection *= -1;        }        if (y > height - bodyY - 60 || y < bodyY + 70) {            ydirection *= -1;        }        if (mouseX > x && mouseX < x && mouseY > y - bodyY && mouseY < y + bodyY) {            overPeng = true;            if (!locked) {            }        } else {            overPeng = false;        }        if (locked == true) {            x = mouseX;            y = mouseY;        }    }    void strik() {        if (keyPressed) {            if (key == k || key == K) {                value = true;                kleurStrik = color(random(255), random(255), random(255), random(255));            }            if (!keyPressed) {                value = false;                kleurStrik = color(random(255), random(255), random(255), random(255));            }        }        println("value" + value);        println("key pressed" + keyPressed);    // println("keypressed" + keyPressed);    // println("k pressed" + key);    }}',?
