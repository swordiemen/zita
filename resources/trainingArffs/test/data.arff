eight+50+random between 0 and height)        if (Rayy < -100) {            bc1 = color(random(0, 256), random(0, 256), random(0, 256));            Rayy = height + 50 + PApplet.parseInt(random(0, height));            Rayx = random(0, width - 100);        }    }    void talk() {        // make the bird scream        scream.play();    }    void changeColor() {        // make the birds change color        bc1 = color(random(0, 256), random(0, 256), random(0, 256));    }}',?
