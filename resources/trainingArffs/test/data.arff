rintln("controls change back");        }        // change        if (keycount >= 60) {            if (key == CODED) {                if (keyCode == DOWN) {                    shipY = shipY - speedY;                } else {                    shipY = shipY + speedY;                }                if (keyCode == LEFT) {                    shipX = shipX + speedX;                }                if (keyCode == RIGHT) {                    shipX = shipX - speedX;                }            }            if (key == a) {                shipX = shipX + speedX;                shipY = shipY - speedY;            }            if (key == d) {                shipX = shipX - speedX;                shipY = shipY - speedY;                println("turned controls");            }        // changed controls        } else {            if (key == CODED) {                if (keyCode == DOWN) {                    shipY = shipY + speedY;                } else {                    shipY = shipY - speedY;                }                if (keyCode == LEFT) {                    shipX = shipX - speedX;                }                if (keyCode == RIGHT) {                    shipX = shipX + speedX;                }            }            if (key == a) {                shipX = shipX - speedX;                shipY = shipY + speedY;            }            if (key == d) {                shipX = shipX + speedX;                shipY = shipY + speedY;            }        }        keycount = keycount + 1;        println(keycount);        candleA.updatePosition(shipX, shipY);    }}',?
